---
import ProjectCard from './ProjectCard.astro';
import projects from '../data/projects.json';
---

<section id="projects">
  <h2>Projects</h2>
  <div class="projects-grid">
    {projects.map((project) => (
      <ProjectCard
        title={project.title}
        description={project.description}
        tags={project.tags}
        github={project.github}
        live={project.live}
      />
    ))}
  </div>
</section>
